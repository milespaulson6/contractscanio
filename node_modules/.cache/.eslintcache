(Math.round((100 * event.loaded) / event.total));\r\n      })\r\n        .then((response) => {\r\n          setMessage(response.data.message);\r\n          return getFiles();\r\n        })\r\n        .then((files) => {\r\n          setFileInfos(files.data);\r\n        })\r\n        .catch(() => {\r\n          setProgress(0);\r\n          setMessage(\"Could not upload the file!\");\r\n          setCurrentFile(undefined);\r\n        });\r\n  \r\n      setSelectedFiles(undefined);\r\n    };\r\n    \r\n    // useEffect(() => {\r\n    //     getFiles().then((response) => {\r\n    //       setFileInfos(response.data);\r\n    //     });\r\n    //   }, []);\r\n\r\n      return (\r\n        <div style={{marginTop: \"70px\", marginBottom: \"100px\"}}>\r\n          {currentFile && (\r\n            <div className=\"progress mb-3\">\r\n              <div\r\n                className=\"progress-bar progress-bar-info progress-bar-striped\"\r\n                role=\"progressbar\"\r\n                aria-valuenow={progress}\r\n                aria-valuemin=\"0\"\r\n                aria-valuemax=\"100\"\r\n                style={{ width: progress + \"%\" }}\r\n              >\r\n                {progress}%\r\n              </div>\r\n            </div>\r\n          )}\r\n    \r\n          <Dropzone onDrop={onDrop} multiple={false}>\r\n            {({ getRootProps, getInputProps }) => (\r\n              <section>\r\n                <div {...getRootProps({ className: \"dropzoneStyle\" })}>\r\n                  <input {...getInputProps()} />\r\n                  {selectedFiles && selectedFiles[0].name ? (\r\n                    <div className=\"selected-file\">\r\n                      {selectedFiles && selectedFiles[0].name}\r\n                    </div>\r\n                  ) : (\r\n                    \"Drag and drop file here, or click to select file\"\r\n                  )}\r\n                </div>\r\n                <br />\r\n                <br />\r\n                <aside className=\"selected-file-wrapper\">\r\n                  <button\r\n                    className=\"btn btn-success\"\r\n                    disabled={!selectedFiles}\r\n                    onClick={upload}\r\n                  >\r\n                    Upload\r\n                  </button>\r\n                </aside>\r\n              </section>\r\n            )}\r\n          </Dropzone>\r\n    \r\n          <div className=\"alert alert-light\" role=\"alert\">\r\n            {message}\r\n          </div>\r\n    \r\n          {fileInfos.length > 0 && (\r\n            <div className=\"card\">\r\n              <div className=\"card-header\">List of Files</div>\r\n              <ul className=\"list-group list-group-flush\">\r\n                {/* {fileInfos.map((file, index) => (\r\n                  <li className=\"list-group-item\" key={index}>\r\n                    <a href={file.url}>{file.name}</a>\r\n                  </li>\r\n                ))} */}\r\n              </ul>\r\n            </div>\r\n          )}\r\n        </div>\r\n    );\r\n};\r\nexport default UploadFiles;","C:\\Users\\18018\\Downloads\\contracts_fe-master\\contracts_fe-master\\src\\services\\FileUploadService.js",[],[],"C:\\Users\\18018\\Downloads\\contracts_fe-master\\contracts_fe-master\\src\\http-common.js",[],[],"C:\\Users\\18018\\Downloads\\contracts_fe-master\\contracts_fe-master\\src\\components\\TestUpload.js",[],[],{"ruleId":"84","severity":1,"message":"85","line":4,"column":8,"nodeType":"86","messageId":"87","endLine":4,"endColumn":13},{"ruleId":"84","severity":1,"message":"88","line":5,"column":10,"nodeType":"86","messageId":"87","endLine":5,"endColumn":17},{"ruleId":"84","severity":1,"message":"89","line":1,"column":27,"nodeType":"86","messageId":"87","endLine":1,"endColumn":36},"no-unused-vars","'axios' is defined but never used.","Identifier","unusedVar","'API_URL' is defined but never used.","'useEffect' is defined but never used."]